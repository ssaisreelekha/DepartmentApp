<mvc:View controllerName="com.departmentapp.departmentapp.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
        <List
		headerText="Employee Details"
		headerLevel="H2"
		items="{
			path: '/results'
		}" >
		<StandardListItem
			title="{Name}"
			description="{City}"/>
	</List>
    </content>
    </Page>
</mvc:View>